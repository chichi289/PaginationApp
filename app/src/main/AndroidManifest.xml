<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chichi289.paginationapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.chichi289.paginationapp.PaginationApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PaginationApp">
        <activity
            android:name="com.chichi289.paginationapp.views.LauncherActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.chichi289.paginationapp.views.PagingListActivity"
            android:exported="false" />

        <activity
            android:name="com.chichi289.paginationapp.views.PagingListDbActivity"
            android:exported="false" />

        <activity
            android:name="com.chichi289.paginationapp.views.ListActivity"
            android:exported="false" />
    </application>

</manifest>